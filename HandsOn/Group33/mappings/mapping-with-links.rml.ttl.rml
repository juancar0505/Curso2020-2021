	rml:logicalSource [
	  rml:source "../csv/red_recarga_acceso_publico__2020_modificado-with-links.csv";
	  rml:referenceFormulation ql:CSV
	];
	rr:subjectMap [
	  rr:template "http://www.semanticweb.org/RecargaMadrid/classes#Operador/{OPERADOR}";
